{
  "include": ["src"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2023",
    "module": "Preserve",
    "moduleResolution": "bundler",
    "types": ["node"],
    "allowImportingTsExtensions": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "noEmit": true
  }
}
